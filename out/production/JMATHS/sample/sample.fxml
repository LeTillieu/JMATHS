<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.chart.LineChart?>
<?import javafx.scene.chart.NumberAxis?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.ListView?>
<?import javafx.scene.control.TextArea?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.VBox?>

<VBox prefHeight="760.0" prefWidth="1360.0" xmlns="http://javafx.com/javafx/11.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="sample.Controller">
    <children>
        <AnchorPane maxHeight="-1.0" maxWidth="-1.0" prefHeight="760.0" prefWidth="1360.0" VBox.vgrow="ALWAYS">
         <children>
            <TextArea fx:id="functionArea" prefHeight="41.0" prefWidth="680.0" />
            <ListView layoutX="680.0" prefHeight="292.0" prefWidth="680.0" />
            <Button layoutY="41.0" mnemonicParsing="false" onAction="#evaluate" prefHeight="25.0" prefWidth="680.0" text="Evaluate" />
            <LineChart fx:id="plot" layoutX="4.0" layoutY="292.0" prefHeight="400.0" prefWidth="1366.0">
              <xAxis>
                  <NumberAxis autoRanging="true" label="x" lowerBound="-10" side="BOTTOM" tickUnit="1" upperBound="10" />
              </xAxis>
              <yAxis>
                  <NumberAxis autoRanging="false" label="y" lowerBound="-10" side="LEFT" tickUnit="1" upperBound="10" />
              </yAxis>
            </LineChart>
         </children>
        </AnchorPane>
    </children>
</VBox>
